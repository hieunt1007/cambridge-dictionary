{
  "manifest_version": 2,
  "name": "Double click plugin for cambridge dictionary",
  "description": "Double click plugin for cambridge dictionary",
  "version": "1.3.1",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Double click plugin for cambridge dictionary",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "\u003Call_urls>"
      ],
      "js": [
        "index.js"
      ],
      "css": [
        "index.css"
      ],
      "all_frames": true
    }
  ]
}